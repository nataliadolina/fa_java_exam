<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="160.0" prefWidth="142.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="com.example.exam_var_37.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label id="from" fx:id="from" prefHeight="17.0" prefWidth="45.0" text="From" />
      <TextField id="fromField" fx:id="fromField" />
      <Label id="to" fx:id="to" text="To" />
      <TextField id="toField" fx:id="toField" />
      <Label id="result" fx:id="result" text="Result" />
      <TextArea id="resultField" fx:id="resultField" prefHeight="200.0" prefWidth="200.0" />
      <Button id="resultBtn" fx:id="resultBtn" mnemonicParsing="false" onAction="#showResult" text="Show result" />
   </children>
</VBox>
